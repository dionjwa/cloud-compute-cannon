{
  "name": "docker-cloud-compute",
  "version": "0.4.0",
  "author": "Dion Amago Whitehead <dion@transition9.com>",
  "scripts": {
    "preview-github-docs": "docker run -it --rm -p 8081:8081 -v ${PWD}:/data -v ~/.grip:/.grip mbentley/grip --context=dionjwa/docker-cloud-compute README.md 0.0.0.0:8081",
    "build": "./bin/compile",
    "build:metaframe": "./bin/build-metaframe",
    "clean": "./bin/clean",
    "reload:metapage": "docker-compose -f docker-compose.tools.yml run metapage-livereload",
    "test": "./bin/install && ./bin/test",
    "test-scaling": "./bin/install && ./bin/test-scaling",
    "template.fluent.conf": "sed -e 's/host ::SERVER_HOSTNAME_PRIVATE::/host elasticsearch/' -e 's/::FLUENTD_WORKER_LOG_TAG_PREFIX::/docker.dockercloudcompute.worker/' -e 's/::FLUENTD_NODEJS_BUNYAN_TAG_PREFIX::/docker.nodejs-bunyan/' etc/log/fluent.conf.template > etc/log/fluent.conf",
    "set-build:server": "rm -f build.hxml && ln -s etc/hxml/server-build.hxml build.hxml",
    "set-build:client-dashboard": "rm -f build.hxml && ln -s clients/dashboard/build-dev.hxml build.hxml",
    "set-build:client-metaframe": "rm -f build.hxml && ln -s clients/metaframe/build-dev.hxml build.hxml",
    "set-build:lambda-autoscaling": "rm -f build.hxml && ln -s src/lambda-autoscaling/build.hxml build.hxml",
    "set-build:test-server-scaling": "rm -f build.hxml && ln -s test/services/local-scaling-server/build.hxml build.hxml",
    "set-build:test-server": "rm -f build.hxml && ln -s test/services/stand-alone-tester/build.hxml build.hxml",
    "set-build:all": "rm -f build.hxml && ln -s etc/hxml/build-all.hxml build.hxml",
    "set-build:all-servers": "rm -f build.hxml && ln -s etc/hxml/build-all-servers.hxml build.hxml"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dionjwa/docker-cloud-compute.git"
  },
  "bugs": {
    "url": "https://github.com/dionjwa/docker-cloud-compute/issues"
  },
  "dependencies": {
    "aws-sdk": "^2.128.0",
    "base64-stream": "^0.1.3",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "bull": "^3.2.0",
    "bull-arena": "^2.2.0",
    "bunyan": "^1.8.5",
    "bunyan-format": "^0.2.1",
    "bunyan-request": "^1.2.0",
    "busboy": "^0.2.14",
    "chokidar": "^1.6.1",
    "cli-color": "^1.1.0",
    "commander": "^2.9.0",
    "cors": "^2.8.1",
    "dev-null": "^0.1.1",
    "dockerode": "^2.5.1",
    "dotenv": "^4.0.0",
    "elasticsearch": "^13.3.1",
    "eventemitter3": "^2.0.3",
    "express": "^4.16.1",
    "express-bunyan-logger": "^1.3.1",
    "fluent-logger": "^2.4.3",
    "fs-extended": "^0.2.1",
    "http-proxy": "^1.16.2",
    "material-ui": "^1.0.0-beta.27",
    "memwatch-next": "^0.3.0",
    "metaframe": "0.0.2",
    "moment-timezone": "^0.5.11",
    "node-uuid": "^1.4.7",
    "node-vagrant": "^1.2.0",
    "nodemon": "^1.12.1",
    "pkgcloud": "^1.4.0",
    "redis": "^2.8.0",
    "request": "^2.83.0",
    "serve-favicon": "^2.4.5",
    "serve-index": "^1.9.1",
    "shortid": "^2.2.6",
    "source-map-support": "^0.5.0",
    "ssh2": "^0.5.5",
    "streamifier": "^0.1.1",
    "tar-fs": "^1.16.0",
    "tar-stream": "^1.5.2",
    "tar.gz": "^1.0.5",
    "terminal-kit": "^1.13.13",
    "through2": "^2.0.3",
    "ws": "^3.2.0"
  },
  "optionalDependencies": {
    "bufferutil": "^3.0.2"
  },
  "devDependencies": {
    "express-livereload": "^0.0.24",
    "haxe-modular": "^0.10.9",
    "react-hot-loader": "^3.1.3",
    "redis-mock": "^0.20.0",
    "webpack": "^3.11.0",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "d3": "^4.13.0",
    "envify": "^4.1.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "haxe-loader": "^0.7.3",
    "history": "^4.7.2",
    "livereloadx": "^0.3.10",
    "material-ui": "^0.20.0",
    "radium": "^0.22.1",
    "random-sentence": "^1.0.4",
    "react": "^16.3.1",
    "react-dnd": "^2.6.0",
    "react-dnd-html5-backend": "^2.6.0",
    "react-dom": "^16.3.1",
    "react-grid-layout": "^0.16.6",
    "react-icons": "^2.2.7",
    "react-infinite": "^0.12.1",
    "react-rnd": "^7.4.0",
    "react-router": "^4.2.0",
    "react-svg": "^2.2.16",
    "react-tap-event-plugin": "^3.0.2",
    "react-virtualized": "^9.18.5",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "shx": "^0.2.2",
    "style-loader": "^0.20.3",
    "uglify-js": "^3.3.20",
    "webpack-dev-server": "^2.11.2"
  },
  "preferGlobal": true,
  "private": false
}
